{
    "name": "codeat3/blade-microns",
    "description": "A package to easily make use of \"Microns Icons\" in your Laravel Blade views. ",
    "keywords": ["Blade", "microns", "Laravel"],
    "homepage": "https://github.com/codeat3/blade-microns",
    "license": "MIT",
    "authors": [
        {
            "name": "Swapnil Sarwe",
            "homepage": "https://swapnilsarwe.com"
        },
        {
            "name": "Dries Vints",
            "homepage": "https://driesvints.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "codeat3/phpcs-styles": "dev-main",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "symfony/finder": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Codeat3\\BladeMicrons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Codeat3\\BladeMicrons\\BladeMicronsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix"
    }
}
